{
    "appId": "com.semaphore.athlytiq",
    "releases": [
        {
            "version": "3.0.0",
            "date": "2023-04-01"
        },
        {
            "version": "2.4.1",
            "date": "2022-11-22"
        }
    ],
    "lowercaseAppName": "feeel",
    "runtimeVersion": "22.08",
    "linuxReleaseBundleDirPath": "../build/linux/x64/release/bundle",
    "linuxArmReleaseBundleDirPath": "../build/linux/aarch64/release/bundle",
    "appDataPath": "com.semaphore.athlytiq.appdata.xml",
    "desktopPath": "com.semaphore.athlytiq.desktop",
    "icons": {
        "symbolic": "icons/com.semaphore.athlytiq-symbolic.svg",
        "24x24": "icons/24x24/com.semaphore.athlytiq.png",
        "32x32": "icons/32x32/com.semaphore.athlytiq.png",
        "48x48": "icons/48x48/com.semaphore.athlytiq.png",
        "128x128": "icons/128x128/com.semaphore.athlytiq.png",
        "256x256": "icons/256x256/com.semaphore.athlytiq.png",
        "512x512": "icons/512x512/com.semaphore.athlytiq.png"
    },
    "buildCommandsAfterUnpack": [
        "ln -s /usr/lib/x86_64-linux-gnu/libsqlite3.so.0 /app/lib/libsqlite3.so",
        "if [ ! -e '/app/lib/libsqlite3.so' ]; then ln -s -f /usr/lib/aarch64-linux-gnu/libsqlite3.so.0 /app/lib/libsqlite3.so; fi",
        "if [ $(arch)=\"aarch64\"  ]; then sed -si \"/Exec=/cExec=env LIBGL_ALWAYS_SOFTWARE=1 feeel\" feeel/com.semaphore.athlytiq.desktop; fi"
    ],
    "finishArgs": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--socket=pulseaudio"
    ],
    "githubReleaseOrganization": "EnjoyingFOSS",
    "githubReleaseProject": "feeel"
}